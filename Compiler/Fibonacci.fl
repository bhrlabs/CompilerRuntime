let
	integer n, x, y, z, u;
	fun fib(integer a;)
		if a = 1
		then
		a := a - 1;
		return a;
		else
		skip;
		fi;
		if a = 2
		then
		a := a - 1;
		return a;
		else
		skip;
		fi;
		z := a - 1;
		u := a - 2;
		x := call fib(z);
		y := call fib(u);
		z := x + y;
		return z;
	end_fun
in
	n := 6;
	x := call fib(n);
	write x;
end